{
    "manifest_version": 3,
    "name": "todo list",
    "version": "0.1.0",
    "description": "todo list with a timer.",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "32": "images/icon32.png",
            "48": "images/icon48.png"
        }
    },
    "permissions": [
        "tabs",
        "storage"
    ]
}